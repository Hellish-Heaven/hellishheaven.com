{
  "name": "hellishheaven.com",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.7.0",
    "@japa/preset-adonis": "^1.1.0",
    "@japa/runner": "^2.0.9",
    "@symfony/webpack-encore": "^2.1.0",
    "@types/react": "^18.0.12",
    "adonis-preset-ts": "^2.1.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "pino-pretty": "^8.0.0",
    "postcss-loader": "^6.0.0",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.24",
    "typescript": "~4.6",
    "youch": "^3.2.0",
    "youch-terminal": "^2.1.4"
  },
  "dependencies": {
    "@adonisjs/ally": "^4.1.4",
    "@adonisjs/auth": "^8.2.1",
    "@adonisjs/bouncer": "^2.3.0",
    "@adonisjs/core": "^5.8.3",
    "@adonisjs/lucid": "^18.0.1",
    "@adonisjs/mail": "^8.1.2",
    "@adonisjs/repl": "^3.1.11",
    "@adonisjs/session": "^6.4.0",
    "@adonisjs/shield": "^7.0.11",
    "@adonisjs/view": "^6.1.5",
    "@zakodium/adonis-mongodb": "^0.13.0",
    "adonis-react": "^0.2.1",
    "proxy-addr": "^2.0.7",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21"
  }
}
